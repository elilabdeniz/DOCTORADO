 @Article{zambranoAl:ijseke2010,
 author = {Zambrano, Arturo  and Gordillo, Silvia  and Johan Fabry},
 title = {A Fine Grained Aspect Coordination Mechanism},
 journal = {International Journal of Software Engineering and Knowledge Engineering (IJSEKE)},
 year = {2010},
